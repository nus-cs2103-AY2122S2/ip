    <children>
        <Label fx:id="dialog" style="-fx-background-color: cyan; -fx-background-radius: 10; -fx-border-color: black; -fx-border-radius: 10; -fx-border-width: 2; -fx-label-padding: 5 10 5 10;" text="Label" wrapText="true">
