  <children>
    <Label fx:id="label" text="Label" wrapText="true" maxWidth="360.0">
      <font>
        <Font name="Consolas" size="14.0" />
      </font>
      <padding>
        <Insets topRightBottomLeft="12.0" />
      </padding>
    </Label>
    <ImageView fx:id="imageView" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" />
  </children>
  <padding>
    <Insets bottom="10.0" left="6.0" top="10.0" right="6.0" />
  </padding>
</fx:root>