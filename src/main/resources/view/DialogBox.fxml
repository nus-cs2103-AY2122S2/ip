            <Insets bottom="5.0" left="10.0" right="10.0" />
         </padding>
         <font>
            <Font name="Arial" size="15.0" />
         </font></Label>
        <Circle fx:id="displayPicture" radius="30.0" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
