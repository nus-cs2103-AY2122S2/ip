    <children>
        <Label fx:id="dialog" style="-fx-background-color: #282e3c; -fx-background-radius: 10; -fx-padding: 7;" text="Label" textFill="#988829" wrapText="true"/>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" style="-fx-border-radius: 50%; -fx-scale-x: 0.8; -fx-scale-y: 0.8; -fx-translate-y: -10;" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
