<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="Infinity" minHeight="210" prefWidth="500.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="dialog" maxWidth="350" style="-fx-background-color: #90EE90; -fx-background-radius: 10px; -fx-padding: 10;" text="Label" textFill="black" wrapText="true">
      <HBox.margin>
        <Insets left="3.0" right="3.0" />
      </HBox.margin>
      <font>
        <Font size="12.0" />
      </font>
    </Label>
    <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
      <HBox.margin>
        <Insets left="3.0" right="3.0" />
      </HBox.margin>
    </ImageView>
  </children>
  <padding>
    <Insets bottom="15.0" left="20.0" right="5.0" top="15.0" />
  </padding>
</fx:root>