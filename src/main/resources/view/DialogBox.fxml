<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="chat-row" stylesheets="@style.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <Label fx:id="chatBlank" maxHeight="Infinity" maxWidth="Infinity" prefWidth="50.0" wrapText="true" HBox.hgrow="SOMETIMES" />
        <Label fx:id="chat" lineSpacing="1.0" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" prefWidth="250.0" styleClass="chat-bubble" text="Label" wrapText="true" HBox.hgrow="SOMETIMES">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="4.0" left="10.0" right="10.0" top="4.0" />
         </padding></Label>
      <Label fx:id="chatArrow" layoutX="15.0" layoutY="25.0" maxWidth="20.0" minHeight="-Infinity" minWidth="20.0" prefWidth="20.0" styleClass="chat-bubble-arrow" wrapText="true">
         <HBox.margin>
            <Insets right="10.0" top="30.0" />
         </HBox.margin>
         <padding>
            <Insets top="10.0" />
         </padding></Label>
      <Circle fx:id="displayPicture" radius="50.0" stroke="BLACK" strokeType="INSIDE" styleClass="chat-profile-picture" HBox.hgrow="NEVER" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
