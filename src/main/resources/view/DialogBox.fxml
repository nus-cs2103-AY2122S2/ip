<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxWidth="1.7976931348623157E308" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="CENTER" contentDisplay="BOTTOM" ellipsisString="" style="-fx-background-color: aquamarine; -fx-background-radius: 10px; -fx-label-padding: 0 5px 10px 5px; -fx-border-color: black; -fx-border-radius: 10px;" text="Label" textFill="#636359">
         <HBox.margin>
            <Insets top="20.0" />
         </HBox.margin>
         <padding>
            <Insets top="10.0" />
         </padding>
         <font>
            <Font name="Candara Light" size="14.0" />
         </font>
      </Label>
        <ImageView fx:id="displayPicture" fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
