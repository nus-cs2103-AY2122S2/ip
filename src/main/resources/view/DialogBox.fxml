    <children>
        <Label fx:id="dialog" style="-fx-border-width: 2; -fx-border-color: black; -fx-border-insets: 10; -fx-border-radius: 5; -fx-background-color: #BF9270; -fx-background-insets: 10; -fx-background-radius: 5;" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
