    <children>
        <Label fx:id="dialog" style="-fx-background-color: #99aab5; -fx-background-radius: 10px; -fx-padding: 10px; -fx-font-family: Droid Serif;" text="Label" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Wingdings 2" size="12.0" />
         </font></Label>
        <ImageView fx:id="displayPicture" fitHeight="130.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="1.0" right="15.0" top="15.0" />
    </padding>
</fx:root>
