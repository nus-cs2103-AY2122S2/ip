<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="CENTER_LEFT" depthTest="DISABLE" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="dialog" alignment="CENTER_RIGHT" style="-fx-background-color: #f0fcf5#f0fcf5;" styleClass="dialog" stylesheets="@../style/style.css" text="Label" wrapText="true">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin></Label>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
